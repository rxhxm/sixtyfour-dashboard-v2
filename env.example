# Langfuse API Keys
LANGFUSE_PUBLIC_KEY=your_langfuse_public_key
LANGFUSE_SECRET_KEY=your_langfuse_secret_key
LANGFUSE_HOST=https://us.cloud.langfuse.com

# Supabase (Optional - if not configured, hardcoded mappings will be used)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Authentication
NEXTAUTH_SECRET=your_secret_key_here_generate_with_openssl
NEXTAUTH_URL=https://your-domain.vercel.app

# PostHog API (for Platform Access Management)
POSTHOG_PERSONAL_API_KEY=your_posthog_personal_api_key
POSTHOG_PROJECT_ID=your_posthog_project_id
POSTHOG_HOST=https://app.posthog.com
